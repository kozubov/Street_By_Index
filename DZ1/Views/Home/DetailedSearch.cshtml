@model IEnumerable<DZ1.Models.Street>

@{
    ViewBag.Title = "DetailedSearch";
}

<h2>Таблица улиц по индексу: @ViewBag.Name_index</h2>

<p>
    @Html.ActionLink("Таблица индексов", "Index", null, new { @class = "index_table_linc" })
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td></td>
        </tr>
    }

</table>
